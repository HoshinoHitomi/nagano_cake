<h1>Admin::Customers#index</h1>
<p>Find me in app/views/admin/customers/index.html.erb</p>

<table>
  <tr>
    <th>会員ID</th>
    <th>氏名</th>
    <th>メールアドレス</th>
    <th>ステータス</th>
  </tr>

  <% @customers.each do |customer| %>
  <tr>
    <td><%= customer.id %></td>
    <td><%= link_to customer.name, admin_customer_path(customer) %></td>
    <td><%= customer.email %></td>
    <td><%= customer.is_active %></td>
  </tr>
  <% end %>
</table>